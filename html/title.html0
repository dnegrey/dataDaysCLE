<slide class="title-slide">
    <!-- The content of this hgroup is replaced programmatically through the slide_config.json. -->
    <hgroup>
        <h1 data-config-title><!-- populated from slide_config.json --></h1>
        <h2 data-config-subtitle><!-- populated from slide_config.json --></h2>
        <h3 id="title-date">
            <script>
                var weekdays = [
                    "Sunday",
                    "Monday",
                    "Tuesday",
                    "Wednesday",
                    "Thursday",
                    "Friday",
                    "Saturday"
                ];
                var months = [
                    "January",
                    "February",
                    "March",
                    "April",
                    "May",
                    "June",
                    "July",
                    "August",
                    "September",
                    "October",
                    "November",
                    "December"
                ];
                var currentDate = new Date();
                var weekDay = weekdays[currentDate.getDay()];
                var monthName = months[currentDate.getMonth()];
                var dayNumber = currentDate.getDate();
                var yearNumber = currentDate.getFullYear();
                var dateOut = weekDay + ", " + monthName + " " + dayNumber + 
                ", " + yearNumber;
                document.write(dateOut);
            </script>
        </h3>
    </hgroup>
    <div class="pdlogo">
        <img id="pdlogo-title" src="data:image/png;base64," alt="Precision Dialogue"/>
    </div>
    <div id="pdinfo">
        <p>
            905 Corporate Way<br>
            Westlake, OH 44145<br>
            877.332.9222
        </p>
        <a href="http://precisiondialogue.com/" style="position:relative; z-index:1;">www.precisiondialogue.com</a>
    </div>
</slide>
